{
  "cache": false,
  "compact": false,
  "check-coverage": false,
  "extension": [".ts"],
  "include": ["src/**/*.ts"],
  "exclude": [
    "instrumented/**",
    "coverage/**",
    "node_modules/**",
    "**/*.d.ts",
    "**/*.test.ts"
  ],
  "require": ["ts-node/register"],
  "sourceMap": true,
  "reporter": ["html", "text", "text-summary"],
  "all": true,
  "instrument": true
}
